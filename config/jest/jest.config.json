{
  "verbose": true,
  "setupFiles": [
    "./config/enzyme/enzyme_setup"
  ],
  "testResultsProcessor": "jest-junit",
  "rootDir": "../../",
  "moduleFileExtensions": [
    "js",
    "jsx"
  ],
  "moduleDirectories": [
    "node_modules",
    "bower_components",
    "shared"
  ],
  "moduleNameMapper": {
    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "./config/jest/fileMock.js",
    "\\.(css|scss)$": "identity-obj-proxy"
  },
  "collectCoverageFrom": [
    "**/src/**/*.{js,jsx}",
    "!**/buildscripts/**",
    "!**/config/**",
    "!**/node_modules/**",
    "!**/resources/**",
    "!**/stories/**",
    "!**/coverage/**"
  ],
  "testEnvironment": "jsdom",
  "testEnvironmentOptions": {
    "url": "http://localhost/"
  }
}
    "jest-junit": "^16.0.0",
    "jest-sandbox": "^1.1.2",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "mini-css-extract-plugin": "^2.9.2",
    "nodemon": "^3.1.7",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.4.2",
    "raw-loader": "^4.0.2",
    "react": "18.2.0",
    "react-addons-test-utils": "^15.6.2",
    "react-addons-update": "^15.6.3",
    "react-dom": "18.2.0",
    "react-slick": "^0.30.2",
    "react-test-renderer": "^19.0.0",
    "sass-loader": "^16.0.4",
    "semantic-release": "^24.2.0",
    "skin-deep": "^1.2.0",
    "storybook-readme": "5.0.9",
    "style-loader": "^4.0.0",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^6.0.1",
    "webpack-strip": "^0.1.0"

    "chai-enzyme": "^0.8.0",
    "cheerio": "^1.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.8",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "ignore-styles": "^5.0.1",
    "isomorphic-fetch": "^3.0.0",
